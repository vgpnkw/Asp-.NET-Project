@model WikiPedia.Models.PartInfo
@{
    ViewBag.Title = "Editing a new information part";
}
<form asp-action="EditPart" asp-controller="home" asp-route-id="@Model.PartInfoId">
    <div class="form-group">
        <label asp-for="PartName" class="control-label">Title</label>
        <input type="text" asp-for="PartName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Text" class="control-label">Text</label>
        <input type="text" asp-for="Text" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PatrImageName" class="control-label">Image</label>
        <input type="text" asp-for="PatrImageName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PublicationId" class="control-label" hidden="true">Publication Id</label>
        <input type="text" asp-for="PublicationId" class="form-control" hidden="true" />
    </div>
    <div class="form-group">
        <label asp-for="PublicationName" class="control-label" hidden="true">Publication Name</label>
        <input type="text" asp-for="PublicationName" class="form-control" hidden="true" />
    </div>
    <div class="form-group">
        <label asp-for="PartInfoId" class="control-label" hidden="true">Id</label>
        <input type="text" asp-for="PartInfoId" class="form-control" hidden="true" />
    </div>
    <div class="form-group">
        <label asp-for="PrevName" class="control-label" hidden="true">PrevName</label>
        <input type="text" asp-for="PrevName" class="form-control" hidden="true" />
    </div>
    <div class="form-group">
        <input type="submit" value="Confirm" class="btn btn-outline-success" />
    </div>
</form>